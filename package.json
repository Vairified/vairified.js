{
  "name": "vairified",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for the Vairified Partner API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check src tests",
    "lint:fix": "biome check --write src tests",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vairified",
    "pickleball",
    "rating",
    "api",
    "sdk",
    "typescript"
  ],
  "author": {
    "name": "Vivek Joshy",
    "email": "vjoshy@vairified.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vairified/vairified.js.git"
  },
  "bugs": {
    "url": "https://github.com/Vairified/vairified.js/issues"
  },
  "homepage": "https://vairified.github.io/vairified.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.0.10",
    "msw": "^2.12.7",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
